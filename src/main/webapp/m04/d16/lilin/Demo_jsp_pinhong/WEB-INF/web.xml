<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Demo_jsp_pinhong</display-name>
  <welcome-file-list>
    <welcome-file>ServletInit</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>ServletInit</servlet-name>
    <servlet-class>com.dt59.servlet.ServletInit</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletInit</servlet-name>
    <url-pattern>/ServletInit</url-pattern>
  </servlet-mapping>
  <filter>
  	<filter-name>setChar</filter-name>
  	<filter-class>com.dt59.util.SetCharUtil</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>setChar</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
</web-app>